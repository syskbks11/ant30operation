<?xml version="1.0" encoding="UTF8" ?>

<!-- This file is ser7ToTime.xml to make time.dat from ser7.dat -->
<!-- time.dat is used by the trk45 library. -->
<!-- ser7.dat can be found at http://maia.usno.navy.mil/ser7/ser7.dat. -->

<regrex>
	<start>
		<line value="#This file was generated by RegrexTextFilter with ser7ToTime.xml. "/>
	</start>

	<replace before="^\s+(\d{4})\s+(\d{1})\s+(\d{1})\s+(\d{5})\s+(-?[\d\.]+)\s+(-?[\d\.]+)\s+([-\s]0\.\d{3})\d{2}\s+$" after="$10$20$3, $4, 34, $7" />
	<replace before="^\s+(\d{4})\s+(\d{1})\s+(\d{2})\s+(\d{5})\s+(-?[\d\.]+)\s+(-?[\d\.]+)\s+([-\s]0\.\d{3})\d{2}\s+$" after="$10$2$3, $4, 34, $7" />
	<replace before="^\s+(\d{4})\s+(\d{2})\s+(\d{1})\s+(\d{5})\s+(-?[\d\.]+)\s+(-?[\d\.]+)\s+([-\s]0\.\d{3})\d{2}\s+$" after="$1$20$3, $4, 34, $7" />
	<replace before="^\s+(\d{4})\s+(\d{2})\s+(\d{2})\s+(\d{5})\s+(-?[\d\.]+)\s+(-?[\d\.]+)\s+([-\s]0\.\d{3})\d{2}\s+$" after="$1$2$3, $4, 34, $7" />

	<replace before="^\s.+$" after="#" />
</regrex>
